{
	"publisher": "zeroMaster",
	"name": "react-use-state-snippet",
	"displayName": "react-use-state-snippet",
	"keywords": [
		"snippets",
		"react",
		"redux",
		"react-native",
		"javascript",
		"typescript"
	],
	"repository": {
		"url": "https://github.com/SuperLeeK/react-use-state"
	},
	"description": "can make functional snippet",
	"version": "0.0.7",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Snippets"
	],
	"extensionKind": [
		"ui",
		"workspace"
	],
	"activationEvents": [
		"onCommand:extension.reactUseState",
		"onCommand:extension.useStateSnippet"
	],
	"keybindings": [
		{
			"command": "extension.useStateSnippet",
			"key": "ctrl+u ctrl+s",
			"mac": "cmd+u cmd+s",
			"when": "editorTextFocus"
		}
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.reactUseState",
				"title": "use State Setter"
			},
			{
				"command": "extension.useStateSnippet",
				"title": "use State Snippet"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/snippet.json"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"clipboardy": "^2.3.0"
	}
}
